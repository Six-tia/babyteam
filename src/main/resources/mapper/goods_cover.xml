<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.edutia.babyteam.dao.GoodsCoverDao">
    <select id = "findByGoodsId" parameterType="Long" resultType="com.edutia.babyteam.entity.GoodsCover">
            select * from t_goods_cover where goods_id = ${id} order by gc_order
    </select>
</mapper>